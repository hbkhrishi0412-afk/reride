{
  "name": "reride-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "A modern vehicle marketplace platform.",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"vite\"",
    "dev:vercel": "vercel dev",
    "dev:api": "node dev-api-server.js",
    "dev:full": "concurrently \"npm run dev:api\" \"npm run dev\"",
    "dev:clean": "npm run clean && vite",
    "dev:fast": "npm run clean && vite --force",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "build:stats": "ANALYZE=true vite build",
    "preview": "vite preview",
    "seed:all": "node seed-all-mock-data.js",
    "seed:users": "node seed-all-mock-data.js",
    "clean": "node -e \"const fs = require('fs'); const path = require('path'); const dirs = ['node_modules/.vite', 'dist']; dirs.forEach(d => { try { fs.rmSync(d, { recursive: true, force: true }); console.log('Cleaned:', d); } catch(e) {} });\"",
    "perf": "node performance-optimize.js",
    "perf:clean": "node performance-optimize.js --clean",
    "perf:analyze": "node performance-optimize.js --analyze",
    "verify-deployment": "node scripts/verify-deployment.js",
    "verify-deployment:local": "node scripts/verify-deployment.js http://localhost:5173",
    "test-categories": "node scripts/test-category-sync.js",
    "test-categories:local": "node scripts/test-category-sync.js http://localhost:5173",
    "db:verify": "node verify-production-setup.js",
    "db:setup": "node scripts/setup-database-collections.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "cap:sync": "npx cap sync",
    "cap:sync:android": "npx cap sync android",
    "cap:open:android": "npx cap open android",
    "cap:build": "npm run build && npx cap sync",
    "cap:build:android": "npm run build && npx cap sync android"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@supabase/supabase-js": "^2.91.0",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "dompurify": "^3.0.8",
    "dotenv": "^17.2.3",
    "express": "^4.21.0",
    "express-rate-limit": "^7.1.5",
    "framer-motion": "^12.23.24",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^7.13.0",
    "react-window": "^1.8.8",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "validator": "^13.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@capacitor/android": "^8.0.1",
    "@capacitor/cli": "^8.0.1",
    "@capacitor/core": "^8.0.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.14.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-window": "^1.8.8",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vercel/node": "^5.3.24",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "axe-playwright": "^2.2.2",
    "baseline-browser-mapping": "^2.9.11",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "lint-staged": "^15.2.0",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.5.6",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.18",
    "terser": "^5.44.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-pwa": "^1.1.0",
    "workbox-window": "^7.3.0"
  }
}
